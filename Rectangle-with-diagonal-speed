color c = color(0);
float x = 20;
float y= 100;
float speed = 1;

void setup() {
  size (500,500);
}
void draw(){
  background(255);
  move();
  display();
}
void move(){
  x=x+speed;
  if(x>width){
    x=0;
  }
  { y=x+speed;
  if(y>width){
    y=0;}
  }
  }
  void display(){
    fill(c);
    rect(x,y,30,10);
    x=y+speed*2;
  }
